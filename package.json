{
  "name": "fps",
  "description": "Flickr Photo Stream web-app",
  "author": "Andrei Tiurin <nrei.tiurin@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn husky install",
    "dev": "NODE_OPTIONS='--inspect' yarn next dev",
    "build": "yarn next build",
    "start": "yarn next start",
    "ts-check": "yarn tsc --project tsconfig.json --pretty --noEmit",
    "lint": "yarn lint:ts . && yarn lint:scss **/*.scss",
    "lint:ts": "yarn eslint --ext ts,tsx",
    "lint:ts:fix": "yarn eslint --ext ts,tsx --fix",
    "lint:scss": "yarn stylelint **/*.scss",
    "lint:scss:fix": "yarn stylelint **/*.scss --fix",
    "test": "yarn jest"
  },
  "browserslist": [
    "defaults",
    "IE 10"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "next": "10.2.0",
    "next-connect": "^0.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.15.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.32.12",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-z-index-value-constraint": "^1.1.0",
    "typescript": "^4.2.4"
  }
}
